CC = @CC@

LEX = @LEX@

SHELL=/bin/sh

@SET_MAKE@

CFLAGS = @CFLAGS@ @DEFS@ -I. -I.. @WMF_CFLAGS@ -DTTFDIR=\"$(libdir)/mswordview/ttf\"

