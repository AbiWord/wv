CC = @CC@
OBJEXT = @OBJEXT@

LEX = @LEX@

SHELL=/bin/sh

@SET_MAKE@

prefix=@prefix@
srcdir=@srcdir@
datadir=@datadir@

HAVE_GLIB = @HAVE_GLIB@

CFLAGS = @CFLAGS@ @DEFS@ -I$(srcdir)/ -I$(srcdir)/.. -DTTFDIR=\"$(datadir)/wv/ttf\" -I$(srcdir)/../libole2 @WMF_CFLAGS@ @GLIB_CFLAGS@
ifneq ($(HAVE_GLIB), 1)
CFLAGS += -I$(srcdir)/../glib-wv/
endif

SHELL = /bin/sh

.SUFFIXES: .C .$(OBJEXT)

MYOBJECTS =  oledecod.$(OBJEXT)

all: oledecod.$(OBJEXT)

%.$(OBJEXT): $(srcdir)/%.c 
	$(CC) $(CFLAGS) -c $(srcdir)/$*.c 

clean:	
	rm -rf $(MYOBJECTS) core 

realclean:  
	rm -rf config.cache config.status config.log Makefile *.$(OBJEXT) core demo

