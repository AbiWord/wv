CC = @CC@
OBJEXT = @OBJEXT@

LEX = @LEX@

SHELL=/bin/sh

@SET_MAKE@

srcdir=@srcdir@
datadir=@datadir@

CFLAGS = @CFLAGS@ @DEFS@ -I$(srcdir)/ -I$(srcdir)/.. -DTTFDIR=\"$(datadir)/wv/ttf\" -I$(srcdir)/../libole2 -I$(srcdir)/../glib-wv @WMF_CFLAGS@ 

SHELL = /bin/sh

.SUFFIXES: .C .$(OBJEXT)

MYOBJECTS =  oledecod.$(OBJEXT)

all: oledecod.$(OBJEXT)

%.$(OBJEXT): $(srcdir)/%.c 
	$(CC) $(CFLAGS) -c $(srcdir)/$*.c 

clean:	
	rm -rf $(MYOBJECTS) core 

realclean:  
	rm -rf config.cache config.status config.log Makefile *.$(OBJEXT) core demo

