EXEEXT = @EXEEXT@

LDFLAGS = @LDFLAGS@

CC = @CC@

SHELL=/bin/sh

RANLIB = @RANLIB@

AR = ar

srcdir=@srcdir@

CFLAGS = @CFLAGS@ -I$(srcdir)/../xmltok/ -I$(srcdir)/

SHELL = /bin/sh

.SUFFIXES: .C .o

PARSEOBJECTS =	\
	xmlparse.o \
	hashtable.o

all: libxmlparse.a

libxmlparse.a:	$(PARSEOBJECTS)
	$(AR) rc $@ $(PARSEOBJECTS) 
	$(RANLIB) $@

%.o: $(srcdir)/%.c
	$(CC) $(CFLAGS) -c $(srcdir)/$*.c

clean: 
	rm -rf $(PARSEOBJECTS) 

distclean: clean
	rm -rf Makefile *.a 

realclean: distclean

