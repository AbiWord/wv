EXEEXT = @EXEEXT@

LDFLAGS = @LDFLAGS@

CC = @CC@

SHELL=/bin/sh

RANLIB = @RANLIB@

AR = ar

srcdir=@srcdir@

# If you know what your system's byte order is, define XML_BYTE_ORDER:
# use -DXML_BYTE_ORDER=12 for little-endian byte order;
# use -DXML_BYTE_ORDER=21 for big-endian (network) byte order.
# -DXML_NS adds support for checking of lexical aspects of XML namespaces spec
# -DXML_MIN_SIZE makes a smaller but slower parser
CFLAGS = @CFLAGS@ -I$(srcdir)/../xmltok/ -I$(srcdir)/ -DXML_NS

SHELL = /bin/sh

.SUFFIXES: .C .o

PARSEOBJECTS =	\
	xmlparse.o \
	hashtable.o

all: libxmlparse.a

libxmlparse.a:	$(PARSEOBJECTS)
	$(AR) rc $@ $(PARSEOBJECTS) 
	$(RANLIB) $@

%.o: $(srcdir)/%.c
	$(CC) $(CFLAGS) -c $(srcdir)/$*.c

clean: 
	rm -rf $(PARSEOBJECTS) 

distclean: clean
	rm -rf Makefile *.a 

realclean: distclean

#  DO  NOT  DELETE THIS LINE -- make depend depends on it.
