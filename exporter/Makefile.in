CC = @CC@
OBJEXT = @OBJEXT@
EXEEXT= @EXEEXT@
LDFLAGS= @LDFLAGS@
SHELL=/bin/sh
LIBS =  @MAGICK_LIBS@ @LIB_PNG@ @ZLIB_LIBS@ @LIBS@ @WMF_LIBS@ @TTF_LIBS@ @XPM_LIBS@ @X_LIBS@ @X11@ @X_EXTRA_LIBS@ -lm

@SET_MAKE@

srcdir=@srcdir@

HAVE_GLIB = @HAVE_GLIB@

CFLAGS = -I$(srcdir)/ -I$(srcdir)/.. @CPPFLAGS@ @CFLAGS@ @DEFS@ @GLIB_CFLAGS@
ifneq ($(HAVE_GLIB), 1)
CFLAGS += -I$(srcdir)/../glib-wv/
endif

SHELL = /bin/sh

.SUFFIXES: .C .$(OBJEXT)
:g/\.0/s//\.$(OBJEXT)/g


MYOBJECTS = \
	anld.$(OBJEXT) \
	anlv.$(OBJEXT) \
	asumy.$(OBJEXT) \
