	bte.$(OBJEXT) \
	bx.$(OBJEXT) \
	chp.$(OBJEXT) \
	dcs.$(OBJEXT) \
	dogrid.$(OBJEXT) \
	dop.$(OBJEXT) \
	doptypography.$(OBJEXT) \
	dttm.$(OBJEXT) \
	fbse.$(OBJEXT) \
	fdoa.$(OBJEXT) \
	ffn.$(OBJEXT) \
	fib.$(OBJEXT) \
	filetime.$(OBJEXT) \
	fld.$(OBJEXT) \
	font.$(OBJEXT) \
	fopt.$(OBJEXT) \
	frd.$(OBJEXT) \
	fspa.$(OBJEXT) \
	ftxbxs.$(OBJEXT) \
	generic.$(OBJEXT) \
	lfo.$(OBJEXT) \
	lspd.$(OBJEXT) \
	lst.$(OBJEXT) \
	lvl.$(OBJEXT) \
