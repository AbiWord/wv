	tc.$(OBJEXT) \
	tlp.$(OBJEXT) \
	wkb.$(OBJEXT) \
	wvexporter.$(OBJEXT) \
	exptest.$(OBJEXT)

all: $(MYOBJECTS)

exptest$(EXEEXT): exptest.$(OBJEXT) ../libwv.a
	$(CC) $(CFLAGS) -o $@ exptest.$(OBJEXT) -L. -L.. -lwv $(LIB) $(LDFLAGS)

clean:	
	rm -rf $(MYOBJECTS) core exptest

distclean: realclean

realclean:
	rm -rf Makefile *.$(OBJEXT) core 

