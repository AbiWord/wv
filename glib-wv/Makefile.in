CC = @CC@
OBJEXT = @OBJEXT@

SHELL=/bin/sh

@SET_MAKE@

srcdir=@srcdir@

HAVE_GLIB = @HAVE_GLIB@

CFLAGS = -I$(srcdir)/ -I$(srcdir)/.. @CPPFLAGS@ @CFLAGS@ @DEFS@ @GLIB_CFLAGS@
ifneq ($(HAVE_GLIB), 1)
CFLAGS += -I$(srcdir)/../glib-wv/
endif

SHELL = /bin/sh

.SUFFIXES: .C .$(OBJEXT)
:g/\.0/s//\.$(OBJEXT)/g


MYOBJECTS = garray.$(OBJEXT) glist.$(OBJEXT) gmem.$(OBJEXT) gmessages.$(OBJEXT) gslist.$(OBJEXT) gstrfuncs.$(OBJEXT) gtree.$(OBJEXT) 

all: $(MYOBJECTS) 

clean:	
	rm -rf $(MYOBJECTS) core 

realclean:  
	rm -rf config.cache config.status config.log Makefile *.$(OBJEXT) core demo

