CC = @CC@
OBJEXT = @OBJEXT@
LEX = @LEX@

SHELL=/bin/sh

@SET_MAKE@

srcdir=@srcdir@

HAVE_GLIB = @HAVE_GLIB@

CFLAGS = @CPPFLAGS@ @CFLAGS@ @DEFS@ -I$(srcdir)/ -I$(srcdir)/.. -DTTFDIR=\"$(datadir)/wv/ttf\" -I$(srcdir)/../libole2 @WMF_CFLAGS@ @GLIB_CFLAGS@
ifneq ($(HAVE_GLIB), 1)
CFLAGS += -I$(srcdir)/../glib-wv/
endif

SHELL = /bin/sh

.SUFFIXES: .C .$(OBJEXT)

MYOBJECTS = \
	ms-ole.$(OBJEXT) \
	ms-ole-summary.$(OBJEXT)

all: $(MYOBJECTS) 

ms-ole.$(OBJEXT): $(srcdir)/ms-ole.c 
	$(CC) $(CFLAGS) -c $(srcdir)/ms-ole.c 

ms-ole-summary.$(OBJEXT): $(srcdir)/ms-ole-summary.c
	$(CC) $(CFLAGS) -c $(srcdir)/ms-ole-summary.c

clean:	
	rm -rf $(MYOBJECTS) core 

realclean:  
	rm -rf Makefile *.$(OBJEXT) core demo

